    <storeData>
        <name>K105</name>
        <specs>
            <combination filterCategory="weights" filterSpec="weight" filterSpecMin="500" filterSpecMax="600"/>
        </specs>
        <functions>
            <function>$l10n_function_fertilizer</function>
        </functions>
        <image>$data/vehicles/bredal/k105/store_k105.png</image>
        <price>45000</price>
        <lifetime>600</lifetime>
        <rotation>0</rotation>
        <brand>BREDAL</brand>
        <category>fertilizerSpreaders</category>
        <shopTranslationOffset>0 -0.007 0</shopTranslationOffset>
        <shopRotationOffset>-0.627 0 0</shopRotationOffset>
        <vertexBufferMemoryUsage>1789440</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>507392</indexBufferMemoryUsage>
        <textureMemoryUsage>917504</textureMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_fertilizerSpreader</typeDesc>
        <filename>$data/vehicles/bredal/k105/k105.i3d</filename>
        <size width="3.1" length="6.95" height="4.2" lengthOffset="-0.8"/>
        <speedLimit value="18" />
        <components maxMass="20500">
            <component centerOfMass="0 1 -1.2" solverIterationCount="10" mass="2675"/>
        </components>
        <schemaOverlay attacherJointPosition="0 0" name="IMPLEMENT" />
        <sounds volumeFactor="1.4" />
    </base>

    <configurationSets title="$l10n_configuration_extension">
        <configurationSet name="$l10n_configuration_valueNone">
            <configuration name="fillVolume"   index="1" />
            <configuration name="fillUnit"     index="1" />
            <configuration name="cover"        index="1" />
        </configurationSet>
        <configurationSet name="$l10n_configuration_valueSmall">
            <configuration name="fillVolume"   index="2" />
            <configuration name="fillUnit"     index="2" />
        </configurationSet>
        <configurationSet name="$l10n_configuration_valueBig">
            <configuration name="fillVolume"   index="3" />
            <configuration name="fillUnit"     index="3" />
            <configuration name="cover"        index="2" />
        </configurationSet>
    </configurationSets>

    <wheels>
        <wheelConfigurations tireCategories="TRAILER">
            <wheelConfiguration name="$l10n_configuration_valueDefault" price="0" saveId="DEFAULT" numDynamicConfigurations="1">
                <wheels>
                    <wheel dimensions="750_60R30_5" isLeft="true" hasTireTracks="true" hasParticles="true">
                        <physics restLoad="1.4" repr="wheelLeft"  forcePointRatio="0.3" initialCompression="8" suspTravel="0.12" spring="115" damper="50" yOffset="0.02"/>
                        <wheelChock parkingNode="wheelChock01" />
                        <wheelChock parkingNode="wheelChock02" isInverted="true" />
                    </wheel>
                    <wheel dimensions="750_60R30_5" isLeft="false" hasTireTracks="true" hasParticles="true">
                        <physics restLoad="1.4" repr="wheelRight" forcePointRatio="0.3" initialCompression="8" suspTravel="0.12" spring="115" damper="50" yOffset="0.02"/>
                    </wheel>
                </wheels>
            </wheelConfiguration>
        </wheelConfigurations>

        <rimMaterial materialTemplateName="RIM_DEFAULT" />
        <hubMaterial materialTemplateName="BREDAL_RED1" />

        <hubs>
            <hub linkNode="wheelLeft"  filename="$data/shared/wheels/hubs/hub_n10_5.xml" isLeft="true"  scale="0.335 0.335 0.335" offset="-0.0065"/>
            <hub linkNode="wheelRight" filename="$data/shared/wheels/hubs/hub_n10_5.xml" isLeft="false" scale="0.335 0.335 0.335" offset="-0.0065"/>
        </hubs>
    </wheels>

    <dynamicallyLoadedParts>
        <dynamicallyLoadedPart node="0" linkNode="chockSupport01" filename="$data/shared/assets/wheelChocks/wheelChockSupport01.i3d"/>
        <dynamicallyLoadedPart node="0" linkNode="chockSupport02" filename="$data/shared/assets/wheelChocks/wheelChockSupport01.i3d"/>
    </dynamicallyLoadedParts>

    <attachable>
        <inputAttacherJoints>
            <inputAttacherJoint node="attacherJoint" jointType="trailerLow" attacherHeight="0.69"/>
        </inputAttacherJoints>
        <support animationName="moveSupport" />
        <brakeForce force="1.3" maxForce="3.5" maxForceMass="20000"/>
        <airConsumer usage="0.3" />
        <sounds>
            <active file="$data/sounds/tools/trailer/trailerRattle_02.wav" innerRadius="10" outerRadius="50" volumeScale="4.3" isLocalSound="true" pitchScale="1.2">
                <volume indoor="0.7" outdoor="0.9" >
                    <modifier type="WHEEL_SUSPENSION" value="-0.04" modifiedValue="0.3"/>
                    <modifier type="WHEEL_SUSPENSION" value="-0.02" modifiedValue="0.2"/>
                    <modifier type="WHEEL_SUSPENSION" value="-0.01" modifiedValue="0.1"/>
                    <modifier type="WHEEL_SUSPENSION" value="-0.00" modifiedValue="0.03"/>
                    <modifier type="WHEEL_SUSPENSION" value="0.01" modifiedValue="0.1"/>
                    <modifier type="WHEEL_SUSPENSION" value="0.02" modifiedValue="0.2"/>
                    <modifier type="WHEEL_SUSPENSION" value="0.04" modifiedValue="0.3"/>

                    <modifier type="SPEED" value="0.0" modifiedValue="0.0"/>
                    <modifier type="SPEED" value="2.0" modifiedValue="1.0"/>
                    <modifier type="SPEED" value="40.0" modifiedValue="1.0"/>
                </volume>
                <lowpassCutoffFrequency indoor="250" outdoor="20000" />
                <lowpassGain indoor="0.5" outdoor="1" />
            </active>
        </sounds>
    </attachable>

    <powerTakeOffs>
        <input inputAttacherJointIndices="1" detachNode="ptoDetachNode" inputNode="ptoInputNode" aboveAttacher="true" />
        <sounds>
            <turnedOn template="tractorPTO" volumeScale="0.5" linkNode="attacherJoint"/>
        </sounds>
    </powerTakeOffs>

    <connectionHoses>
        <hose inputAttacherJointIndices="1" type="hydraulicIn"  node="hydraulicIn01" adapterNode="hydraulicIn01_connector" length="1.5" diameter="0.02" straighteningFactor="2">
            <objectChange node="hydraulicIn01_detached" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <hose inputAttacherJointIndices="1" type="hydraulicOut" node="hydraulicOut01" adapterNode="hydraulicOut01_connector" length="1.5" diameter="0.02" straighteningFactor="2">
            <objectChange node="hydraulicOut01_detached" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <hose inputAttacherJointIndices="1" type="hydraulicIn"  node="hydraulicIn02" adapterNode="hydraulicIn02_connector" length="1.5" diameter="0.02" straighteningFactor="2">
            <objectChange node="hydraulicIn02_detached" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <hose inputAttacherJointIndices="1" type="hydraulicOut" node="hydraulicOut02" adapterNode="hydraulicOut02_connector" length="1.5" diameter="0.02" straighteningFactor="2">
            <objectChange node="hydraulicOut02_detached" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <hose inputAttacherJointIndices="1" type="hydraulicIn"  node="hydraulicIn03" adapterNode="hydraulicIn03_connector" length="1.5" diameter="0.02" straighteningFactor="2">
            <objectChange node="hydraulicIn03_detached" visibilityActive="false" visibilityInactive="true"/>
        </hose>
        <hose inputAttacherJointIndices="1" type="electric" node="electric" adapterNode="electric_connector" length="1.5" diameter="0.02" straighteningFactor="1.5">
            <objectChange node="electric_detached" visibilityActive="false" visibilityInactive="true"/>
        </hose>
    </connectionHoses>

    <powerConsumer ptoRpm="400" neededMaxPtoPower="10"/>

    <lights>
        <!-- small -->
        <sharedLight linkNode="backlightLeft"     filename="$data/shared/assets/lights/lizard/rearLight10_left.xml" />
        <sharedLight linkNode="backlightRight"    filename="$data/shared/assets/lights/lizard/rearLight10_right.xml" />
        <sharedLight linkNode="markerLeft"        filename="$data/shared/assets/reflectors/lizard/yellowRectangle_01.xml" />
        <sharedLight linkNode="markerRight"       filename="$data/shared/assets/reflectors/lizard/yellowRectangle_01.xml" />
        <sharedLight linkNode="bigTriangle"       filename="$data/shared/assets/reflectors/lizard/bigTriangle.xml" />
        <sharedLight linkNode="triangle01"        filename="$data/shared/assets/reflectors/lizard/redTriangle_01.xml" />
        <sharedLight linkNode="triangle02"        filename="$data/shared/assets/reflectors/lizard/redTriangle_01.xml" />
        <!-- big -->
        <sharedLight linkNode="backlightLeftBig"  filename="$data/shared/assets/lights/lizard/rearLight10_left.xml" />
        <sharedLight linkNode="backlightRightBig" filename="$data/shared/assets/lights/lizard/rearLight10_right.xml" />
        <sharedLight linkNode="markerLeftBig"     filename="$data/shared/assets/reflectors/lizard/yellowRectangle_01.xml" />
        <sharedLight linkNode="markerRightBig"    filename="$data/shared/assets/reflectors/lizard/yellowRectangle_01.xml" />
        <sharedLight linkNode="bigTriangleBig"    filename="$data/shared/assets/reflectors/lizard/bigTriangle.xml" />
        <sharedLight linkNode="triangle01Big"     filename="$data/shared/assets/reflectors/lizard/redTriangle_01.xml" />
        <sharedLight linkNode="triangle02Big"     filename="$data/shared/assets/reflectors/lizard/redTriangle_01.xml" />

        <sharedLight linkNode="rearLightOvalOrange_01_01" lightTypes="0" filename="$data/shared/assets/lights/lizard/rearLight26Orange.xml" />
        <sharedLight linkNode="rearLightOvalOrange_01_02" lightTypes="0" filename="$data/shared/assets/lights/lizard/rearLight26Orange.xml" />
        <sharedLight linkNode="rearLightOvalWhite_01_01"  lightTypes="0" filename="$data/shared/assets/lights/lizard/rearLight26White.xml" />
        <sharedLight linkNode="rearLightOvalWhite_01_02"  lightTypes="0" filename="$data/shared/assets/lights/lizard/rearLight26White.xml" />

        <realLights>
            <high>
                <!-- small -->
                <light node="backLightsHigh1" lightTypes="0"/>
                <turnLightLeft node="turnLightLeftBack"/>
                <turnLightRight node="turnLightRightBack"/>
                <brakeLight node="backLightsHigh1"/>
                <!-- big -->
                <light node="backLightsHigh1_ws" lightTypes="0"/>
                <turnLightLeft node="turnLightLeftBack_ws"/>
                <turnLightRight node="turnLightRightBack_ws"/>
                <brakeLight node="backLightsHigh1_ws"/>
            </high>
        </realLights>
    </lights>

    <licensePlates>
        <licensePlate node="licensePlateBack01" position="BACK" preferedType="ELONGATED" placementArea="0.1 0.3 0.1 0.3"/>
        <licensePlate node="licensePlateBack02" position="BACK" preferedType="SQUARISH" placementArea="0.11 0.22 0.11 0.22"/>
    </licensePlates>

    <sprayer fillUnitIndex="1" unloadInfoIndex="1">
        <usageScales workingWidth="24" scale="1" />

        <effects>
            <!-- unloading spreader -->
            <effectNode effectNode="effectUnloadLeft"  delay="0" materialType="unloading" fadeTime="1"/>
            <effectNode effectNode="effectUnloadRight" delay="0" materialType="unloading" fadeTime="1"/>

            <!-- unloading big spreader -->
            <effectNode effectNode="effectRight" delay="0.3" materialType="unloading" fadeTime="1"/>
            <effectNode effectNode="effectLeft"  delay="0.3" materialType="unloading" fadeTime="1"/>
        </effects>

        <animationNodes>
            <animationNode node="diskSmallDesign01Left"   rotSpeed="900"  turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="diskSmallDesign01Right"  rotSpeed="-900" turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="diskSmallDesign02Left"   rotSpeed="900"  turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="diskSmallDesign02Right"  rotSpeed="-900" turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="diskWideDesign01Left"    rotSpeed="900"  turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="diskWideDesign01Right"   rotSpeed="-900" turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="diskWideDesign02Left"    rotSpeed="900"  turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="diskWideDesign02Right"   rotSpeed="-900" turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="roller01"    rotAxis="1" rotSpeed="15"   turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="roller02"    rotAxis="1" rotSpeed="-40"  turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="roller03"    rotAxis="3" rotSpeed="400"  turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="roller04"    rotAxis="1" rotSpeed="-40"  turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="roller06"    rotAxis="3" rotSpeed="-40"  turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="roller07"    rotAxis="3" rotSpeed="-40"  turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="roller08"    rotAxis="3" rotSpeed="40"   turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode node="roller09"    rotAxis="3" rotSpeed="40"   turnOnFadeTime="1" turnOffFadeTime="2" />
            <animationNode class="ScrollingAnimation" node="belt"      shaderParameterComponent="2" scrollSpeed="-0.05" turnOnFadeTime="2" turnOffFadeTime="2"/>
            <animationNode class="ScrollingAnimation" node="beltLeft"  shaderParameterComponent="2" scrollSpeed="-0.05" turnOnFadeTime="2" turnOffFadeTime="2"/>
            <animationNode class="ScrollingAnimation" node="beltRight" shaderParameterComponent="2" scrollSpeed="-0.05" turnOnFadeTime="2" turnOffFadeTime="2"/>
        </animationNodes>

        <!-- different spray type depending on the current folding configuration and fill type (lime can only be spreaded up to 18m and fertilizer up to 30m) -->
        <sprayTypes>
            <!-- small fertilizer -->
            <sprayType foldingConfigurationIndex="1" fillTypes="fertilizer unknown">
                <usageScales workingWidth="24" />

                <effects>
                    <effectNode effectClass="FertilizerMotionPathEffect" effectType="FERTILIZER" linkNode="fertilizerEffectLeftSmall">
                        <motionPathEffect textureFilename="$data/vehicles/bredal/fertilizerEffectLeftSmallArray.dds" numRows="16" rowLength="32" minFade="0.03" isLeft="true"/>
                    </effectNode>
                    <effectNode effectClass="FertilizerMotionPathEffect" effectType="FERTILIZER" linkNode="fertilizerEffectRightSmall">
                        <motionPathEffect textureFilename="$data/vehicles/bredal/fertilizerEffectRightSmallArray.dds" numRows="16" rowLength="32" minFade="0.03" isLeft="false"/>
                    </effectNode>
                </effects>

                <ai>
                    <areaMarkers leftNode="aiMarkerSmallFertilizerLeft" rightNode="aiMarkerSmallFertilizerRight" backNode="aiMarkerSmallFertilizerBack" width="24"/>
                    <sizeMarkers leftNode="sizeMarkerSmallLeft" rightNode="sizeMarkerSmallRight" backNode="sizeMarkerSmallBack" />
                    <collisionTrigger node="aiCollisionNodeSmall" width="3.0" height="3.1"/>
                </ai>

                <objectChange node="diskSmallDesign02Left" visibilityActive="true" visibilityInactive="false"/>
                <objectChange node="diskSmallDesign02Right" visibilityActive="true" visibilityInactive="false"/>
            </sprayType>

            <!-- small lime -->
            <sprayType foldingConfigurationIndex="1" fillTypes="lime">
                <usageScales workingWidth="12" />

                <effects>
                    <effectNode effectClass="FertilizerMotionPathEffect" effectType="FERTILIZER" linkNode="limeEffectLeftSmall">
                        <motionPathEffect textureFilename="$data/vehicles/bredal/limeEffectLeftSmallArray.dds" numRows="12" rowLength="32" minFade="0.03" isLeft="true" delay="0.5"/>
                    </effectNode>
                    <effectNode effectClass="FertilizerMotionPathEffect" effectType="FERTILIZER" linkNode="limeEffectRightSmall">
                        <motionPathEffect textureFilename="$data/vehicles/bredal/limeEffectRightSmallArray.dds" numRows="12" rowLength="32" minFade="0.03" isLeft="false" delay="0.5"/>
                    </effectNode>
                    <effectNode effectNode="effectNodeSmallLimeParticleSmoke" delay="0.5" effectClass="ParticleEffect" particleType="smoke" materialType="smokeParticle" emitCountScale="5" worldSpace="true"/>
                </effects>

                <ai>
                    <areaMarkers leftNode="aiMarkerSmallLimeLeft" rightNode="aiMarkerSmallLimeRight" backNode="aiMarkerSmallLimeBack" width="12" />
                    <sizeMarkers leftNode="sizeMarkerSmallLeft" rightNode="sizeMarkerSmallRight" backNode="sizeMarkerSmallBack" />
                    <collisionTrigger node="aiCollisionNodeSmall" width="3.0" height="3.1"/>
                </ai>

                <objectChange node="diskSmallDesign01Left" visibilityActive="true" visibilityInactive="false"/>
                <objectChange node="diskSmallDesign01Right" visibilityActive="true" visibilityInactive="false"/>
            </sprayType>

            <!-- big fertilizer -->
            <sprayType foldingConfigurationIndex="2" fillTypes="fertilizer unknown">
                <usageScales workingWidth="30" />

                <effects>
                    <effectNode effectClass="FertilizerMotionPathEffect" effectType="FERTILIZER" linkNode="fertilizerEffectLeftWide">
                        <motionPathEffect textureFilename="$data/vehicles/bredal/fertilizerEffectWideArray.dds" numRows="20" rowLength="32" minFade="0.03" isLeft="true"/>
                    </effectNode>
                    <effectNode effectClass="FertilizerMotionPathEffect" effectType="FERTILIZER" linkNode="fertilizerEffectRightWide">
                        <motionPathEffect textureFilename="$data/vehicles/bredal/fertilizerEffectWideArray.dds" numRows="20" rowLength="32" minFade="0.03" isLeft="false"/>
                    </effectNode>
                </effects>

                <ai>
                    <areaMarkers leftNode="aiMarkerBigFertilizerLeft" rightNode="aiMarkerBigFertilizerRight" backNode="aiMarkerBigFertilizerBack" width="30"/>
                    <sizeMarkers leftNode="sizeMarkerBigLeft" rightNode="sizeMarkerBigRight" backNode="sizeMarkerBigBack" />
                    <collisionTrigger node="aiCollisionNodeBig" width="7.4" height="3.1"/>
                </ai>

                <objectChange node="diskWideDesign02Left" visibilityActive="true" visibilityInactive="false"/>
                <objectChange node="diskWideDesign02Right" visibilityActive="true" visibilityInactive="false"/>
            </sprayType>

            <!-- big lime -->
            <sprayType foldingConfigurationIndex="2" fillTypes="lime">
                <usageScales workingWidth="18" />

                <effects>
                    <effectNode effectClass="FertilizerMotionPathEffect" effectType="FERTILIZER" linkNode="limeEffectLeftWide">
                        <motionPathEffect textureFilename="$data/vehicles/bredal/limeEffectWideArray.dds" numRows="12" rowLength="24" minFade="0.03" isLeft="true" delay="0.8"/>
                    </effectNode>
                    <effectNode effectClass="FertilizerMotionPathEffect" effectType="FERTILIZER" linkNode="limeEffectRightWide">
                        <motionPathEffect textureFilename="$data/vehicles/bredal/limeEffectWideArray.dds" numRows="12" rowLength="24" minFade="0.03" isLeft="false" delay="0.8"/>
                    </effectNode>
                    <effectNode effectNode="effectNodeBigLeftLimeParticleSmoke" delay="0.8" effectClass="ParticleEffect" particleType="smoke" materialType="smokeParticle" emitCountScale="5" worldSpace="true"/>
                    <effectNode effectNode="effectNodeBigRightLimeParticleSmoke" delay="0.8" effectClass="ParticleEffect" particleType="smoke" materialType="smokeParticle" emitCountScale="5" worldSpace="true"/>
                </effects>

                <ai>
                    <areaMarkers leftNode="aiMarkerBigLimeLeft" rightNode="aiMarkerBigLimeRight" backNode="aiMarkerBigLimeBack" width="18"/>
                    <sizeMarkers leftNode="sizeMarkerBigLeft" rightNode="sizeMarkerBigRight" backNode="sizeMarkerBigBack" />
                    <collisionTrigger node="aiCollisionNodeBig" width="7.4" height="3.1"/>
                </ai>

                <objectChange node="diskWideDesign01Left" visibilityActive="true" visibilityInactive="false"/>
                <objectChange node="diskWideDesign01Right" visibilityActive="true" visibilityInactive="false"/>
            </sprayType>
        </sprayTypes>
    </sprayer>

    <workAreas>
        <workArea type="sprayer" functionName="processSprayerArea" disableBackwards="false" requiresGroundContact="false" sprayType="1">
            <area startNode="workAreaStartSmallFertilizer" widthNode="workAreaWidthSmallFertilizer" heightNode="workAreaHeightSmallFertilizer" />
            <onlyActiveWhenLowered value="false"/>
        </workArea>
        <workArea type="sprayer" functionName="processSprayerArea" disableBackwards="false" requiresGroundContact="false" sprayType="2">
            <area startNode="workAreaStartSmallLime" widthNode="workAreaWidthSmallLime" heightNode="workAreaHeightSmallLime" />
            <onlyActiveWhenLowered value="false"/>
        </workArea>
        <workArea type="sprayer" functionName="processSprayerArea" disableBackwards="false" requiresGroundContact="false" sprayType="3">
            <area startNode="workAreaStartBigFertilizer" widthNode="workAreaWidthBigFertilizer" heightNode="workAreaHeightBigFertilizer" />
            <onlyActiveWhenLowered value="false"/>
        </workArea>
        <workArea type="sprayer" functionName="processSprayerArea" disableBackwards="false" requiresGroundContact="false" sprayType="4">
            <area startNode="workAreaStartBigLime" widthNode="workAreaWidthBigLime" heightNode="workAreaHeightBigLime" />
            <onlyActiveWhenLowered value="false"/>
        </workArea>
    </workAreas>

    <foldable>
        <foldingConfigurations title="$l10n_configuration_spreaderSystem">
            <foldingConfiguration name="$l10n_configuration_valueSpreadingDiscs" price="0" workingWidth="12"/>
            <foldingConfiguration name="$l10n_configuration_value6mSpreadingUnit" price="8000" workingWidth="18">
                <foldingParts startMoveDirection="1" turnOnFoldMaxLimit="0.1" turnOnFoldMinLimit="0">
                    <foldingPart animationName="folding" speedScale="0.4"/>
                </foldingParts>

                <objectChange node="smallSpreader" visibilityInactive="true" visibilityActive="false" />
                <objectChange node="wideSpreader"  visibilityInactive="false" visibilityActive="true" />

                <objectChange node="colPartBigLeft"   compoundChildInactive="false" compoundChildActive="true" />
                <objectChange node="colPartBigRight"  compoundChildInactive="false" compoundChildActive="true" />
            </foldingConfiguration>
        </foldingConfigurations>
    </foldable>

    <animations>
        <animation name="toggleCover">
            <part node="bigCoverRot" startTime="0" endTime="3" startRot="0 0 0" endRot="0 0 90" />
            <part node="smallCoverRot" startTime="0" endTime="3" startRot="0 0 0" endRot="0 0 90" />

            <sound template="hydraulicOpen02" startTime="0.01" endTime="2.99" volumeScale="0.8" pitchScale="1.6"/>
            <sound template="clackVar4"             startTime="0.01" direction="1"  volumeScale="1"   pitchScale="1.8" />
            <sound template="clackVar6"             startTime="2.99" direction="1"  volumeScale="1.3" pitchScale="0.8" />
            <sound template="cylinderPunch"         startTime="2.99" direction="1"  volumeScale="0.3" pitchScale="1.5" />
            <sound template="thudVar1"              startTime="2.99" direction="1"  volumeScale="1"   pitchScale="0.7" />
            <sound template="clackVar5"             startTime="2.99" direction="-1" volumeScale="1"   pitchScale="1" />
            <sound template="metalClose"            startTime="0.01" direction="-1" volumeScale="1.4" pitchScale="1.4" />
            <sound template="cylinderPunch"         startTime="0.01" direction="-1" volumeScale="0.3" pitchScale="1.6" />
            <sound template="clackVar6"             startTime="0.01" direction="-1" volumeScale="1.2" pitchScale="0.6" />
        </animation>
        <animation name="turnOnAnimation">
            <part node="flapArm" startTime="0" endTime="1" startRot="0 0 0" endRot="0 0 30" />
            <part node="flap" startTime="0" endTime="1" startRot="0 0 0" endRot="0 0 -30" />

            <sound template="latchSmall" startTime="0.01" direction="-1" volumeScale="0.2" pitchScale="0.7" linkNodeOffset="0 1 -3"/>
            <sound template="slideOpen"  startTime="0.22" direction="1"  volumeScale="3.5" pitchScale="1.5" linkNodeOffset="0 1 -3"/>
            <sound template="slideClose" startTime="0.99" direction="-1" volumeScale="3.5" pitchScale="2"   linkNodeOffset="0 1 -3"/>
            <sound template="metalClose" startTime="0.99" direction="1"  volumeScale="0.8" pitchScale="2"   linkNodeOffset="0 1 -3"/>
            <sound template="clackVar6"  startTime="0.99" direction="1"  volumeScale="1.3" pitchScale="1"   linkNodeOffset="0 1 -3"/>
            <sound template="metalClose" startTime="0.01" direction="-1" volumeScale="0.8" pitchScale="1.8" linkNodeOffset="0 1 -3"/>
        </animation>
        <animation name="moveSupport">
            <part node="supportArm" startTime="0" endTime="0.5" startTrans="0.104 0.458 -0.297" endTrans="0.104 0.01 -0.297" />
            <part node="ladder" startTime="0.5" endTime="1.0" startRot="30 0 0" endRot="0 0 0" />
